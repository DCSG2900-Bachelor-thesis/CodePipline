# Configure the scope of the scan
targetSite=https://example.com
targetScopeIncludeURLs=true
targetScopeURLsRegex=^https://example.com/.*

# Enable spidering and active scans
spidering=true
activeScan=true

# Configure passive scan rules
passiveScanRules=X-Content-Type-OptionsMissing;X-Frame-OptionsHeaderNotSet
